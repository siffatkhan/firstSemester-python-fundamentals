    "#WHITE ONE CALCULATOR BEST ONE\n",
    "\n",
    "from tkinter import *\n",
    "\n",
    "def click(event):\n",
    "    global stringvalue\n",
    "    text=event.widget.cget(\"text\")\n",
    "    if text==\"=\":\n",
    "        if stringvalue.get().isdigit():\n",
    "            value=int(stringvalue.get())\n",
    "        else:\n",
    "            try:\n",
    "                value=eval(screen.get())\n",
    "            except Exception as e:\n",
    "                value=\"Error\"\n",
    "                \n",
    "            \n",
    "        stringvalue.set(value)\n",
    "        screen.update()\n",
    "    elif text==\"C\":\n",
    "        stringvalue.set(\"\")\n",
    "        screen.update()\n",
    "        \n",
    "    elif text==\"竊申":\n",
    "        pass\n",
    "    else:\n",
    "        stringvalue.set(stringvalue.get()+text)\n",
    "        screen.update()\n",
    "        \n",
    "        \n",
    "\n",
    "root=Tk()\n",
    "root.geometry(\"350x530\")\n",
    "root.config(background=\"white\")\n",
    "root.title(\"Calculator By Siffat\")\n",
    "# root.minsize(350,500)\n",
    "# root.maxsize(350,500)\n",
    "\n",
    "stringvalue=StringVar()\n",
    "stringvalue.set(\"\")\n",
    "screen=Entry(root,textvar=stringvalue,font=\"lucida 60\",highlightthickness=0, bg=\"black\")\n",
    "screen.pack(fill=X, ipadx=20, pady=20, padx=10)\n",
    "\n",
    "\n",
    "frame1=Frame(root, bg=\"white\")\n",
    "button=Button(frame1,text=\"C\",fg=\"black\", padx=8, pady=10,font=\"lucida 35 bold\",borderwidth=0, highlightthickness=0, highlightbackground=root.cget(\"bg\"))\n",
